{
  "name": "route-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf ./app/generated",
    "generate:grpc": "mkdirp ./app/generated && protoc -I=./proto stationapi.proto --js_out=import_style=commonjs,binary:./app/generated --grpc-web_out=import_style=typescript,mode=grpcweb:./app/generated",
    "generate": "npm run clean && npm run generate:grpc"
  },
  "dependencies": {
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "google-protobuf": "^3.21.2",
    "mkdirp": "^3.0.1",
    "postcss": "^8",
    "protoc-gen-grpc-web": "^1.4.2",
    "protoc-gen-js": "^3.21.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
